<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.agriculturalseedsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgriculturalSeedSystem">
        <activity android:name=".forgotpassword.ForgetPasswordSuccessActivity" />
        <activity android:name=".forgotpassword.SetPasswordActivity" />
        <activity android:name=".forgotpassword.MakeSelectionActivity" />
        <activity android:name=".forgotpassword.VerifyOTP" />
        <activity android:name=".activity.VerifyPhoneActivity" />
        <activity android:name=".activity.ProfileActivity" />
        <activity android:name=".activity.DashboardActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.ForgotPasswordActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>